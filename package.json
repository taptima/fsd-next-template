{
    "name": "fsd-next-template",
    "version": "1.0.2",
    "description": "Skeleton front-end project built on top of Next.js",
    "license": "MIT",
    "author": "Taptima",
    "scripts": {
        "dev": "next --turbo",
        "debug": "NODE_OPTIONS='--inspect' next dev",
        "typecheck": "tsc --noEmit",
        "build": "next build",
        "start": "next start",
        "format": "prettier --write --check",
        "format-all": "pnpm run format \"src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
        "lint-js": "eslint --fix",
        "lint-js-all": "pnpm run lint-js \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint-styles": "stylelint --fix --config .stylelintrc.json",
        "lint-styles-all": "pnpm run lint-styles \"src/**/*.{css,scss}\"",
        "lint-deadcode": "ts-prune --skip \"@types|data/driver/graphql.ts\" --ignore '.next|middleware|config|app|shared/lib/mock|shared/types/graphql|.d.ts|.stories.tsx'",
        "lint": "pnpm run lint-js-all && pnpm run lint-styles-all && pnpm format-all --check",
        "graphql:build": "graphql-codegen --config codegen.yml --verbose",
        "svg:optimize": "svgo --exclude \".*\\.raw\\.svg\"",
        "svg:optimize-all": "pnpm run svg:optimize -f src/shared/assets/icons",
        "storybook:dev": "storybook dev -p 6006 -c ./config/storybook --no-open",
        "storybook:build": "storybook build -c ./config/storybook",
        "test:unit": "jest --config config/jest/jest.config.ts",
        "prepare": "husky"
    },
    "browserslist": [
        "last 1 Chrome major versions",
        "last 2 Firefox major versions",
        "last 2 Safari major versions",
        "last 2 Edge major versions",
        "last 3 Android major versions",
        "last 3 ChromeAndroid major versions",
        "last 2 iOS major versions"
    ],
    "engines": {
        "node": "~20.12.0",
        "pnpm": "^8.7.0"
    },
    "dependencies": {
        "async-mutex": "^0.5.0",
        "axios": "^1.12.0",
        "clsx": "^2.1.1",
        "graphql": "^16.11.0",
        "next": "^15.4.5",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "sharp": "^0.34.3",
        "swr": "^2.3.4",
        "typescript": "^5.9.2",
        "yup": "^1.7.0",
        "zustand": "^4.5.7"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@graphql-codegen/cli": "^5.0.7",
        "@graphql-codegen/typescript": "^4.1.6",
        "@graphql-codegen/typescript-operations": "^4.6.1",
        "@next/bundle-analyzer": "^15.4.5",
        "@storybook/addon-docs": "^9.1.1",
        "@storybook/addon-links": "^9.1.1",
        "@storybook/nextjs": "^9.1.1",
        "@storybook/testing-library": "^0.2.2",
        "@svgr/webpack": "^8.1.0",
        "@testing-library/jest-dom": "^6.6.4",
        "@testing-library/react": "^16.3.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.19.9",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^8.57.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-next": "^15.4.5",
        "eslint-config-prettier": "^9.1.2",
        "eslint-import-resolver-typescript": "^3.10.1",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-no-relative-import-paths": "^1.6.1",
        "eslint-plugin-perfectionist": "^2.11.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-storybook": "^9.1.1",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^15.5.2",
        "loader-utils": "^3.3.1",
        "prettier": "^3.6.2",
        "regenerator-runtime": "^0.14.1",
        "sass": "^1.89.2",
        "storybook": "^9.1.1",
        "stylelint": "^16.23.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "stylelint-order": "^6.0.4",
        "svgo": "^3.3.2",
        "ts-prune": "^0.10.3",
        "webpack": "^5.101.0"
    },
    "pnpm": {
        "patchedDependencies": {
            "next@15.4.6": "patches/next@15.4.6.patch"
        }
    }
}
